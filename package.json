    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.4",
    "cookie-parser": "~1.4.4",
    "coveralls": "^3.1.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.13",
    "nyc": "^15.1.0",
    "pg": "^8.7.1",
    "prettier": "^2.4.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^4.3.0"
